VICE  ?= $(MAINSRC)/vice2
FLASHLIGHT_INC= @FLASHLIGHT_INC@ 
SDIR   = $(VICE)/src/raster
SHLD   = @LD@ -shared
LD     = @LD@ -r
AR     = @AR@
CC     = @CC@
SINC   = $(SDIR)
OBJ	   = ./obj/raster
INC    = 
AINC   = . $(INC) \
$(FLASHLIGHT_INC) \
$(MAINSRC)/common \
$(MAINSRC)/viceport \
$(VICE)/src/arch/unix \
$(VICE)/src \
