VER_MINOR = 20
VERSION = $(VER_MAJOR).$(VER_MINOR)

ALLINC = $(addprefix -I,$(AINC))


CFLAGS = -g @CONF_CFLAGS@ -O3 -pthread -c $(ALLINC)

DIRS= obj

